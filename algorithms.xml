    <algorithm>
        <name>BCEP</name>
        <class>algorithms.bcep.BCEP_Model</class>
        <parameter>
            <name>Minimum support</name>
            <type>real</type>
            <domain>
                <min>0</min>
                <max>1</max>
            </domain>
            <default>0.01</default>
        </parameter>
        <parameter>
            <name>Minimum GrowthRate</name>
            <type>real</type>
            <domain>
                <min>1</min>
                <max>10000</max>
            </domain>
            <default>100</default>
        </parameter>
    </algorithm>

    <algorithm>
        <name>DeEPS</name>
        <class>algorithms.deeps.DeEPS_Wrapper</class>
        <parameter>
            <name>ALPHA</name>
            <type>real</type>
            <domain>
                <min>0</min>
                <max>1</max>
            </domain>
            <default>0.12</default>
        </parameter>
    </algorithm>


    <algorithm>
        <name>iEPMiner</name>
        <class>algorithms.iepminer.IEPMiner</class>
        <parameter>
            <name>Minimum Support</name>
            <type>real</type>
            <domain>
                <min>0</min>
                <max>1</max>
            </domain>
            <default>0.01</default>
        </parameter>
        <parameter>
             <name>Minimum Growth Rate</name>
            <type>real</type>
            <domain>
                <min>0</min>
                <max>10000000</max>
            </domain>
            <default>2</default>
        </parameter>
         <parameter>
             <name>Minimum Chi-Squared</name>
            <type>real</type>
            <domain>
                <min>0</min>
                <max>10000</max>
            </domain>
            <default>3.84</default>
        </parameter>
        
    </algorithm>

    <algorithm>
        <name>Tree-based JEP-C</name>
        <class>algorithms.tree_based_jep.TreeBasedJEP</class>
        <parameter>
            <name>Ordering</name>
            <type>nominal</type>
            <domain>
                <item>frequency</item>
                <item>ratio</item>
                <item>ratioInverse</item>
                <item>LPNC</item>
                <item>MPPC</item>
                <item>hybrid</item>
            </domain>
            <default>6</default>
        </parameter>
        <parameter>
            <name>Alpha</name>
            <type>real</type>
            <domain>
                <min>0</min>
                <max>1</max>
            </domain>
            <default>0.3</default>
        </parameter>
        <parameter>
            <name>Pattern Max Length</name>
            <type>integer</type>
            <domain>
                <min>-1</min>
                <max>99999999</max>
            </domain>
            <default>-1</default>
        </parameter>
    </algorithm>




    <algorithm>
        <name>Top-k minimal SJEPs</name>
        <class>algorithms.topk.TopK</class>
    </algorithm>


    

