            <name>Minimum support</name>
            <type>real</type>
            <domain>
                <min>0</min>
                <max>1</max>
            </domain>
            <default>0.01</default>
        </parameter>
        <parameter>
            <name>Minimum GrowthRate</name>
            <type>real</type>
            <domain>
                <min>1</min>
                <max>10000</max>
            </domain>
            <default>100</default>
        </parameter>
